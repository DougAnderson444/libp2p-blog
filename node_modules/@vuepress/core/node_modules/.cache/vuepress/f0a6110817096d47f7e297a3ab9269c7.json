 return originalPush.call(this, location)\n  }\n\n  if (!isServer) {\n    // track page view via Countly when route changes\n    router.afterEach((to) => {\n      if (!window.Countly) return\n      window.Countly.q.push(['track_pageview', to.path])\n    })\n  }\n\n  Vue.use(Vuex)\n  Vue.use(VScrollLock)\n  Vue.use(VueMq, { breakpoints })\n  Vue.use(VLazyImagePlugin)\n  Vue.use(VueSocialSharing)\n\n  Vue.directive(Transition.name, Transition.directive)\n\n  const store = createStore()\n  Vue.mixin({ store: store })\n}\n"],"mappings":";AAAA,OAAO,oBAAoB;AAC3B,OAAOA,IAAI,MAAM,MAAM;AACvB,SAASC,WAAW,QAAQ,oBAAoB;AAChD,OAAOC,WAAW,MAAM,eAAe;AACvC,OAAOC,KAAK,MAAM,QAAQ;AAC1B,SAASC,gBAAgB,QAAQ,cAAc;AAC/C,OAAOC,gBAAgB,MAAM,oBAAoB;AACjD,OAAOC,UAAU,MAAM,4CAA4C;AAEnE,OAAO,8CAA8C;AAErD,gBAAe,CAAC;EAAEC,GAAG;EAAEC,MAAM;EAAEC,QAAQ;EAAEC;AAAS,CAAC,KAAK;EACtD,MAAM;IAAEC;EAAY,CAAC,GAAGF,QAAQ,CAACG,WAAW;;EAE5C;AACF;AACA;AACA;AACA;AACA;AACA;EACE,MAAMC,YAAY,GAAGL,MAAM,CAACM,IAAI;EAChCN,MAAM,CAACM,IAAI,GAAG,SAASA,IAAI,CAACC,QAAQ,EAAEC,SAAS,EAAEC,QAAQ,EAAE;IACzD,IAAID,SAAS,IAAIC,QAAQ,EACvB,OAAOJ,YAAY,CAACK,IAAI,CAAC,IAAI,EAAEH,QAAQ,EAAEC,SAAS,EAAEC,QAAQ,CAAC;IAC/D,OAAOJ,YAAY,CAACK,IAAI,CAAC,IAAI,EAAEH,QAAQ,CAAC;EAC1C,CAAC;EAED,IAAI,CAACL,QAAQ,EAAE;IACb;IACAF,MAAM,CAACW,SAAS,CAAEC,EAAE,IAAK;MACvB,IAAI,CAACC,MAAM,CAACC,OAAO,EAAE;MACrBD,MAAM,CAACC,OAAO,CAACC,CAAC,CAACT,IAAI,CAAC,CAAC,gBAAgB,EAAEM,EAAE,CAACI,IAAI,CAAC,CAAC;IACpD,CAAC,CAAC;EACJ;EAEAjB,GAAG,CAACkB,GAAG,CAACzB,IAAI,CAAC;EACbO,GAAG,CAACkB,GAAG,CAACvB,WAAW,CAAC;EACpBK,GAAG,CAACkB,GAAG,CAACtB,KAAK,EAAE;IAAEQ;EAAY,CAAC,CAAC;EAC/BJ,GAAG,CAACkB,GAAG,CAACrB,gBAAgB,CAAC;EACzBG,GAAG,CAACkB,GAAG,CAACpB,gBAAgB,CAAC;EAEzBE,GAAG,CAACmB,SAAS,CAACpB,UAAU,CAACqB,IAAI,EAAErB,UAAU,CAACoB,SAAS,CAAC;EAEpD,MAAME,KAAK,GAAG3B,WAAW,EAAE;EAC3BM,GAAG,CAACsB,KAAK,CAAC;IAAED,KAAK,EAAEA;EAAM,CAAC,CAAC;AAC7B,CAAC"}]}